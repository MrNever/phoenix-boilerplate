    "webpack-hot-middleware": "^2.19.1"
  },
  "dependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router": "^4.0.15",
    "babel-polyfill": "^6.26.0",
    "echarts": "^3.7.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "lodash": "^4.17.4",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "moment": "^2.18.1",
