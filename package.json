{
  "name": "phoenix-boilerplate",
  "version": "0.0.1",
  "main": "scripts/start.js",
  "description": "phoenix-boilerplate",
  "scripts": {
    "start": "nodemon --watch scripts/start.js",
    "build": "NODE_ENV=production webpack --config scripts/webpack.production.js --progress && npm run clean && mv _dist build",
    "clean": "rimraf build",
    "test": "npm run lint && jest",
    "lint": "eslint 'app/**/*.{js,jsx}'",
    "precommit": "npm run lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "/build/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "node"
    ],
    "transform": {
      "\\.tsx?$": "./node_modules/typescript-babel-jest",
      "\\.jsx?$": "./node_modules/babel-jest"
    },
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahyiru/phoenix-boilerplate"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.1",
    "colors": "^1.1.2",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "css-loader": "^0.28.7",
    "del": "^3.0.0",
    "enzyme": "^2.9.1",
    "es6-promise": "^4.1.1",
    "eslint": "^4.7.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gulp-imagemin": "^3.3.0",
    "gulp4": "^4.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "imagemin-pngquant": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.1.0",
    "jwt-simple": "^0.5.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "log4js": "^2.3.3",
    "marked": "^0.3.6",
    "mobx-react-devtools": "^4.2.15",
    "mongoose": "^4.11.11",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "opn": "^5.1.0",
    "passport": "^0.4.0",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "request": "^2.81.0",
