    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.22.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cors": "^2.8.2",
    "cron": "^1.2.1",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
